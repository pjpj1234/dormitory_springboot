<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.southwind.mapper.CollegeMapper">
    <select id="findAvailableCollegeId" resultType="java.lang.Integer">
        select id from college where available > 0 limit 0,1
    </select>
</mapper>

